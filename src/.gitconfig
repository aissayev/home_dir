[alias]
	ann = annotate
	br = branch
	ci = commit
	co = checkout
	del = rm
	delete = rm
	di = diff
	get = !git pull && git submodule init && git submodule update
	h = help
	move = mv
	praise = blame
	put = push origin HEAD
	rb = rebase
	remove = rm
	ren = mv
	rename = mv
	rmbr = !sh -c \"git branch -d \\$0 && git push origin :heads/\\$0\"
	st = status
	stat = status
	unci = reset HEAD^
	uncommit = reset HEAD^
	unst = reset HEAD
	unstage = reset HEAD
[branch]
	autosetuprebase = always
	autosetupmerge = true
[branch "master"]
	rebase = true
[color]
	ui = true
[core]
	excludesfile = ~/.gitignore
	whitespace = trailing-space,space-before-tab
[push]
	default = tracking
[rebase]
	autosquash = true
[rerere]
	enabled = true
