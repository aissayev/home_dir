[alias]
	ann = annotate
	br = branch
	ci = commit
	co = checkout
	del = rm
	delete = rm
	di = diff
	h = help
	move = mv
	praise = blame
	rb = rebase
	remove = rm
	ren = mv
	rename = mv
	st = status
	stat = status
[branch]
	autosetuprebase = always
	autosetupmerge = true
[branch "master"]
	rebase = true
[color]
	ui = true
[core]
	whitespace = trailing-space,space-before-tab
	excludesfile = ~/.gitignore
[push]
	default = tracking
[rebase]
	autosquash = true
[rerere]
	enabled = true
